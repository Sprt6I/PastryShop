<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PastryAdmin.Pages.Admin_Login_Page"
             Title="Admin_Login_Page">
    <StackLayout>
        <Label Text="Admin Login"/>
        <Entry x:Name="login_entry" Placeholder="login"/>
        <Entry x:Name="password_entry" Placeholder="password"/>

        <Button Text="Login" Clicked="Login_"/>
    </StackLayout>
</ContentPage>
